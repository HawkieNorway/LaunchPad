@{
    ViewData["Title"] = "Home Page";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/images/LaunchPad-Banner-01.png" alt="{ Launch-Pad }" class="img-responsive" />
            <div class="container">
                <div class="carousel-caption">
                    <p>
                        Script | Schedule | Publish
                        @*<a class="btn btn-default btn-default" href="http://github.com/michaelburns/LaunchPad">
                                Learn More
                            </a>*@
                    </p>
                </div>
            </div>
        </div>
        <div class="item">
            <img src="~/images/LaunchPad-Banner-02.png" alt="Script" class="img-responsive" />
            <div class="container">
                <div class="carousel-caption">
                    <p>
                        Write and share powerful scripts within your teams.
                        @*<a class="btn btn-default btn-default" href="http://github.com/michaelburns/LaunchPad">
                                Learn More
                            </a>*@
                    </p>
                </div>
            </div>
        </div>
        <div class="item">
            <img src="~/images/LaunchPad-Banner-03.png" alt="Schedule" class="img-responsive" />
            <div class="container">
                <div class="carousel-caption">
                    <p>
                        Take control over when and how often your scripts run.
                        @*<a class="btn btn-default btn-default" href="http://github.com/michaelburns/LaunchPad">
                                Learn More
                            </a>*@
                    </p>
                </div>
            </div>
        </div>
        <div class="item">
            <img src="~/images/LaunchPad-Banner-04.png" alt="Publish" class="img-responsive" />
            <div class="container">
                <div class="carousel-caption">
                    <p>
                        Give users the power of automation(Coming Soon).
                        <a class="btn btn-default btn-default" href="http://github.com/michaelburns/LaunchPad">
                                    Learn More
                                </a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-3">
            
        </div>
        <div class="col-md-3">

        </div>
        <div class="col-md-3">

        </div>
        <div class="col-md-3">

        </div>
    </div>
